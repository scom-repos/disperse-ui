{
  "name": "@disperse/main",
  "version": "0.1.1",
  "main": "{root}/main/index.js",
  "env": "mainnet",
  "routes": [
    {
      "url": "#/disperse",
      "module": "{root}/disperse/index.js",
      "default": true,
      "dependencies": {
        "@disperse/disperse-utils": "{root}/disperse-utils/index.js"
      }
    }
  ],
  "dependencies": {
    "@scom/disperse-sdk": "{root}/sdks/index.js",
    "@disperse/assets": "{root}/assets/index.js",
    "@disperse/global": "{root}/global/index.js",
    "@disperse/store": "{root}/store/index.js",
    "@disperse/common": "{root}/common/index.js"
  },
  "params": {
    "projectInfo": {
      "img": "img/sc-footer.svg",
      "versionText": "Version: 0.1.1-Beta"
    }
  }
}